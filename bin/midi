# midi - list available midi devices
#
# todo:
#   - tbd
#

import mido, typer


def main():
    """list available midi devices"""
    typer.echo(f"available midi device(s): {', '.join(set(mido.get_output_names()))}")


if __name__ == "__main__":
    typer.run(main)
